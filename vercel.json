{
    "version": 2,
    "name": "animevideoplatzi-jorgevicuna",
    "builds": [
        { "use": "@vercel/static-build", "src": "package.json"}
    ],
    "routes": [
        {
            "src": "(.*).js",
            "dest": "/$1.js"
        },
        {
            "src": "(.*).json",
            "dest": "/$1.json"
        },
        {
            "src": "(.*).css",
            "dest": "/$1.css"
        },
        {
            "src": "(.*).png",
            "dest": "/$1.png"
        },
        {
            "src": "(.*).jpg",
            "dest": "/$1.jpg"
        },
        {
            "src": "/.*",
            "dest": "index.html"
        }
    ]
  }
  